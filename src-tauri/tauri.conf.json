{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Cove",
  "version": "1.0.0",
  "identifier": "com.officellm.chat",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Cove",
        "label": "main",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "trafficLightPosition": {
          "x": 13,
          "y": 28
        }
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "scope": {
          "allow": [
            "**"
          ]
        }
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["binaries/officellm"],
    "macOS": {
      "entitlements": "entitlements.plist"
    }
  }
}